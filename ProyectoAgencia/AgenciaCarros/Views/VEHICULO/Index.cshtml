@model IEnumerable<AgenciaCarros.Models.VEHICULO>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>VEHICULOS GUARDADOS</h2>

<p>
    @Html.ActionLink("Ingresar nuevo Vehiculo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PLACA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MARCA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MODELO)
        </th>
        <th>
            @Html.DisplayName("AÑO")
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CILINDRAJE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.COLOR)
        </th>
        <th>
            @Html.DisplayName("TIPO DE COMBUSTIBLE")
        </th>
        <th>
            @Html.DisplayName("PRECIO UNITARIO")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PLACA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MARCA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MODELO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ANNO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CILINDRAJE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.COLOR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TIPO_COMBUSTIBLE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PRECIO_UNITARIO)
        </td>
        <td>
            @Html.ActionLink("EDITAR", "Edit", new { id=item.ID_VEHICULO }) |
            @Html.ActionLink("DETALLES", "Details", new { id=item.ID_VEHICULO }) |
            @Html.ActionLink("ELIMINAR", "Delete", new { id=item.ID_VEHICULO })
        </td>
    </tr>
}

</table>
