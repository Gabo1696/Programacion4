@model AgenciaCarros.Models.PROVEEDOR

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ELIMINAR</h2>

<h3>Esta seguro que quiere eliminar este proveedor?</h3>
<div>
    <h4>PROVEEDOR</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("NOMBRE DEL PROVEEDOR")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NOMBRE_PROVEEDOR)
        </dd>

        <dt>
            @Html.DisplayName("DESCRIPCION DEL PROVEEDOR")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DESCRIPCION_PROVEEDOR)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="ELIMINAR" class="btn btn-default" /> |
            @Html.ActionLink("REGRESAR", "Index")
        </div>
    }
</div>
